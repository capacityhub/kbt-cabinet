{
  "name": "mp-admin",
  "version": "4.0.1",
  "description": "admin template",
  "author": "wuhaotx@feinno.com",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "mini-program",
    "element-ui"
  ],
  "dependencies": {
    "area-data": "^5.0.6",
    "axios": "^0.21.1",
    "browserslist": "^4.5.5",
    "clipboard": "^2.0.1",
    "codemirror": "5.45.0",
    "countup": "^1.8.2",
    "cropperjs": "^1.4.1",
    "date-fns": "^1.30.1",
    "dplayer": "^1.25.0",
    "driver.js": "0.9.5",
    "echarts": "^4.1.0",
    "element-ui": "2.8.2",
    "file-saver": "1.3.8",
    "font-awesome": "^4.7.0",
    "gitalk": "^1.4.0",
    "html-webpack-plugin": "^3.2.0",
    "html2canvas": "^1.0.0-alpha.12",
    "iview": "^3.3.0",
    "iview-area": "^1.6.0",
    "js-cookie": "^2.2.0",
    "print-js": "^1.0.54",
    "quill": "^1.3.6",
    "rasterizehtml": "^1.3.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "scss-loader": "0.0.1",
    "tinymce": "^5.0.3",
    "tree-table-vue": "^1.1.0",
    "tui-editor": "^1.3.3",
    "v-contextmenu": "^2.8.0",
    "vue": "^2.5.17",
    "vue-awesome": "^3.1.0",
    "vue-clipboard2": "^0.3.0",
    "vue-cropper": "^0.4.8",
    "vue-i18n": "^5.0.3",
    "vue-lazyload": "^1.2.6",
    "vue-pdf": "^4.0.6",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.1",
    "vue-splitpane": "^1.0.4",
    "vuex": "^3.0.1",
    "xlsx": "^0.13.4"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-template-compiler": "^2.5.17"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
